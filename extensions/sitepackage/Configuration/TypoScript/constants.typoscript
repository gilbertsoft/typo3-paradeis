############
### PAGE ###
############
page {
    logo {
        file = EXT:sitepackage/Resources/Public/Images/logo.svg
        fileInverted = EXT:sitepackage/Resources/Public/Images/logo-inverted.svg
        height = 65
        width = 155
        alt = Paradeis Logo
        linktitle = Home
    }
    fluidtemplate {
        layoutRootPath = EXT:sitepackage/Resources/Private/Layouts/Page/
        partialRootPath = EXT:sitepackage/Resources/Private/Partials/Page/
        templateRootPath = EXT:sitepackage/Resources/Private/Templates/Page/
    }
    preloader {
        enable = 0
        logo.file = EXT:sitepackage/Resources/Public/Images/logo-inverted.svg
        logo.height = 75
    }
    theme {
        navigation {
            style = default
            type = top
        }
        cookieconsent {
            content.href = 69
        }
        copyright.text = Copyright © 1999-2019 Enothek Halde 9 AG
        googleFont.font = Ubuntu
        language {
            enable = 0
            languageValue = 0
        }
        meta.navigationValue = 68,69,70,74
    }
}

[treeLevel = 0]
page {
    theme {
        navigation {
            style = default-transition
            type = top
        }
    }
}
[end]

